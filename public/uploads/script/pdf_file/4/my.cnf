
[mysqladmin]
user=root

[mysqld]
basedir=/home/sandbox/rubystack-2.0.0-30/mysql
datadir=/home/sandbox/rubystack-2.0.0-30/mysql/data
port=3306
socket=/home/sandbox/rubystack-2.0.0-30/mysql/tmp/mysql.sock
