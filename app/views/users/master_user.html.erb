<div id="wrapper">
  <%= render 'layouts/side_bar' %>
  <div id="page-wrapper">
    <%= render 'layouts/messages' %>
    <div class="row">
      <div class="col-md-12">
        <h1>User Admin </h1>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-md-12">
        <%= link_to 'New User', master_registration_users_path , class: 'btn btn-default'  %>
      </div>
    </div>
    <hr>
    <%= form_tag request.path, :method => 'get' do %>
        <% content_tag :label do %>
            Search by Name:
            <%= text_field_tag :search_members, params[:search] %>
        <% end %>
    <% end %>
    <%= render 'form_master_user' %>
  </div>
</div>
