<p id="notice"><%= notice %></p>

<h1>Listing Function Data Types</h1>

<table>
  <thead>
    <tr>
      <th>Function type</th>
      <th>User</th>
      <th>Script</th>
      <th>Description</th>
      <th>Number of registers types</th>
      <th>Number of data types</th>
      <th>Complexity</th>
      <th>Number of function points</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @function_data_types.each do |function_data_type| %>
      <tr>
        <td><%= function_data_type.function_type %></td>
        <td><%= function_data_type.user_id %></td>
        <td><%= function_data_type.script_id %></td>
        <td><%= function_data_type.description %></td>
        <td><%= function_data_type.number_of_registers_types %></td>
        <td><%= function_data_type.number_of_data_types %></td>
        <td><%= function_data_type.complexity %></td>
        <td><%= function_data_type.number_of_function_points %></td>
        <td><%= link_to 'Show', function_data_type %></td>
        <td><%= link_to 'Edit', edit_function_data_type_path(function_data_type) %></td>
        <td><%= link_to 'Destroy', function_data_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Function data type', new_function_data_type_path %>
