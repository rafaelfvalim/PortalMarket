<%= simple_form_for(@reservation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :script_id %>
    <%= f.input :owner_user_id %>
    <%= f.input :reserve_for %>
    <%= f.input :expiration_time %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
