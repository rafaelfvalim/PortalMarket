<table class="table table-striped table-bordered table-hover">
  <thead>
  <th>Name</th>
  <th>CNPJ</th>
  <th></th>
  </thead>
  <tbody>
  <% member_scripts = MemberScript.where(script_id: script.id) %>
  <% members_participation.each do |m| %>
      <tr>
        <td><%= m.company_name %></td>
        <td><%= m.cnpj %></td>
        <td><%= link_to "Add", add_participation_member_wizard_scripts_path(member_id: m.id, script_id: script.id), class: "btn btn-primary btn-xs", remote: true, method: :get, disabled: member_scripts.map(&:member_id).include?(m.id) %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= will_paginate members_participation %>