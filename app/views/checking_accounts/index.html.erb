<%= render 'layouts/messages' %>
<div class="wrapper wrapper-content animated fadeIn">
  <div class="row">
    <div class="col-md-12">
      <h1><%= t 'labels.checking_account.page_header' %></h1>
    </div>
  </div>
  <hr>
  <div class="row">
    <div class="col-md-12">
      <%= link_to :back, class: 'btn btn-primary next' do %>
          <i class="fa fa-chevron-left"> <%= t 'labels.buttons.back'%></i>
     <% end %>
    </div>
  </div>
  <br>
  <h3><%=t 'labels.checking_account.page_data_table_header' %></h3>
  <div class="row" role="contributor_dashboard">
    <div class="col-md-12">
      <div class="panel panel-default center">
        <div class="panel-body">
          <table class="table table-striped table-condensed responsive" id="script-table" data-source="<%= member_ajax_checking_accounts_path(format: :json) %>">
            <thead>
            <tr>
              <th><%= t 'labels.script_attributes.script' %></th>
              <th><%= t 'labels.script_attributes.description'%></th>
              <th><%= t 'labels.script_attributes.description' %></th>
              <th><%= t 'labels.script_attributes.moviment_date' %></th>
              <th><%= t 'labels.script_attributes.status' %></th>
              <th><%= t 'labels.script_attributes.value' %></th>
              <th><%= t 'labels.script_attributes.interest' %></th>
              <th><%= t 'labels.script_attributes.commission' %></th>
              <th><%= t 'labels.script_attributes.currency' %></th>
              <th></th>
              <th></th>
            </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
