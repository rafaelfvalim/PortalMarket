<%= simple_form_for(@license) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :invoice_id %>
    <%= f.input :description %>
    <%= f.input :key %>
    <%= f.input :expiration_date %>
    <%= f.input :valid %>
    <%= f.input :obs %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
