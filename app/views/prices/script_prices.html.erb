<%= render 'layouts/messages' %>
<div class="row wrapper page-heading">
  <div class="col-lg-10">
    <h2><%= t 'labels.script_price.page_header' %>: </h2>
  </div>
</div>

<div class="panel panel-default">
  <div class="panel-body">
    <%= form_tag create_prices_prices_path, method: :put, id: 'create_price_form' do %>
        <div class="row">
          <div class="col-md-6">
            <div class="pull-left">
              <%= link_to "#{t 'labels.script_price.show_prices'}", prices_path, class: 'btn btn-success' %>
            </div>
          </div>
        </div>
        <br>
        <div class="row">
          <div class="col-md-12">
            <div id="script_prices">
              <%= render 'prices/form_script_prices_old' %>
            </div>
          </div>
        </div>
    <% end %>
  </div>
</div>