<%= simple_form_for(@landing_text) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :subtitle %>
    <%= f.input :content %>
    <%= f.input :location_name %>
  </div>
  <div class="form-actions">
    <%= f.submit 'Salvar' , class:'btn btn-primary' %>
  </div>
<% end %>
