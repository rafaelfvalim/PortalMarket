<%= render 'layouts/messages' %>

<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Scripts</h2>
  </div>
</div>

<div class="wrapper wrapper-content animated fadeIn">

  <div class="ibox-content m-b-sm border-bottom">
    <div class="row">
      <div class="col-sm-12">
        <%= link_to new_script_path, :class => 'btn btn-success' do %>
            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> New
        <% end %>
      </div>
    </div>
  </div>


  <div class="row" role="contributor_dashboard">
    <div class="col-md-12">
      <div class="panel panel-default center">
        <div class="panel-body">
          <table class="table table-striped table-condensed responsive" id="script-table" data-source="<%= contributor_members_path(format: :json) %>">
            <thead>
            <tr>
              <th>Code</th>
              <th>Name</th>
              <th>Description</th>
              <th>Platform</th>
              <th>Industry</th>
              <th>Complexity</th>
              <th></th>
              <th></th>
            </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
