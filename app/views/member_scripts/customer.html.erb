<%= render 'layouts/messages' %>
<div class="row wrapper page-heading">
  <div class="col-lg-10">
    <h2><%= t 'labels.menu.my_scripts' %> :: Status <%= params[:status] %></h2>
  </div>
</div>

<div class="wrapper wrapper-content animated fadeIn">
  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-default center">
        <div class="panel-body">
          <table class="table table-striped table-condensed responsive" id="costumer-table" data-source="<%= get_data_grid_customer_ajax_member_scripts_url(status: params[:status], format: :json) %>" width="100%">
            <thead>
            <th>ID</th>
            <th>ID Consumo</th>
            <th>ID Script</th>
            <th>Descrição</th>
            <th>Valor</th>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
