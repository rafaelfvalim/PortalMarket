<%= simple_form_for(@function_transaction_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :script_id %>
    <%= f.input :function_type %>
    <%= f.input :description %>
    <%= f.input :number_of_referenced_files %>
    <%= f.input :number_of_data_types %>
    <%= f.input :complexity %>
    <%= f.input :number_of_function_points %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
