<p id="notice"><%= notice %></p>

<h1>Listing Function Transaction Types</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Script</th>
      <th>Function type</th>
      <th>Description</th>
      <th>Number of referenced files</th>
      <th>Number of data types</th>
      <th>Complexity</th>
      <th>Number of function points</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @function_transaction_types.each do |function_transaction_type| %>
      <tr>
        <td><%= function_transaction_type.user_id %></td>
        <td><%= function_transaction_type.script_id %></td>
        <td><%= function_transaction_type.function_type %></td>
        <td><%= function_transaction_type.description %></td>
        <td><%= function_transaction_type.number_of_referenced_files %></td>
        <td><%= function_transaction_type.number_of_data_types %></td>
        <td><%= function_transaction_type.complexity %></td>
        <td><%= function_transaction_type.number_of_function_points %></td>
        <td><%= link_to 'Show', function_transaction_type %></td>
        <td><%= link_to 'Edit', edit_function_transaction_type_path(function_transaction_type) %></td>
        <td><%= link_to 'Destroy', function_transaction_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Function transaction type', new_function_transaction_type_path %>
