<%= render 'layouts/messages' %>
<div class="wrapper wrapper-content animated fadeIn">
  <div class="row">
    <div class="col-md-12">
      <h2> Status:  <%= @status.description == 'Inicial' ? 'Incompleto' : @status.description %></h2>
    </div>
  </div>
  <hr>
  <!-- /.row -->
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default center">
        <div class="panel-body">
          <table class="table table-striped table-condensed display responsive no-wrap" id="contributors-table" data-source="<%= contributor_scripts_path(status_id: @status.id, format: :json) %>">
            <thead>
            <tr>
              <th><%= t 'labels.script_attributes.code' %></th>
              <th><%= t 'labels.script_attributes.status' %></th>
              <th><%= t 'labels.script_attributes.name' %></th>
              <th><%= t 'labels.script_attributes.platform' %></th>
              <th><%= t 'labels.script_attributes.complexity' %></th>
              <th><%= t 'labels.script_attributes.pdf_file' %></th>
              <th><%= t 'labels.script_attributes.script_file' %></th>
              <th><%= t 'labels.buttons.actions' %></th>
            </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>