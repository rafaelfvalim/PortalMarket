<p id="notice"><%= notice %></p>

<h1>Listing Script Likes</h1>

<table>
  <thead>
    <tr>
      <th>Script</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @script_likes.each do |script_like| %>
      <tr>
        <td><%= script_like.script_id %></td>
        <td><%= script_like.user_id %></td>
        <td><%= link_to 'Show', script_like %></td>
        <td><%= link_to 'Edit', edit_script_like_path(script_like) %></td>
        <td><%= link_to 'Destroy', script_like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Script like', new_script_like_path %>
