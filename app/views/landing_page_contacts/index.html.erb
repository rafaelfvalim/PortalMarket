<p id="notice"><%= notice %></p>

<h1>Listing Landing Page Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Area</th>
      <th>Industry</th>
      <th>Suggestion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landing_page_contacts.each do |landing_page_contact| %>
      <tr>
        <td><%= landing_page_contact.name %></td>
        <td><%= landing_page_contact.phone %></td>
        <td><%= landing_page_contact.area %></td>
        <td><%= landing_page_contact.industry_id %></td>
        <td><%= landing_page_contact.suggestion %></td>
        <td><%= link_to 'Show', landing_page_contact %></td>
        <td><%= link_to 'Edit', edit_landing_page_contact_path(landing_page_contact) %></td>
        <td><%= link_to 'Destroy', landing_page_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landing page contact', new_landing_page_contact_path %>
